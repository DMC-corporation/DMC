<h1>Reponse list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Libelle_masculin</th>
            <th>Libelle_feminin</th>
            <th>Type</th>
            <th>Creation_date</th>
            <th>Update_date</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('reponse_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.libellemasculin }}</td>
            <td>{{ entity.libellefeminin }}</td>
            <td>{{ entity.type }}</td>
            <td>{% if entity.creationdate %}{{ entity.creationdate|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{% if entity.updatedate %}{{ entity.updatedate|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('reponse_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('reponse_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('reponse_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
